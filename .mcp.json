{
  "mcpServers": {
    "ag-mcp": {
      "command": "npx",
      "args": ["ag-mcp"]
    },
    "primeng": {
      "command": "npx",
      "args": ["-y", "@primeng/mcp"]
    },
    "nx-mcp": {
      "command": "npx",
      "args": ["nx", "mcp"]
    },
    "angular-cli": {
      "command": "npx",
      "args": ["-y", "@angular/cli", "mcp"]
    },
    "tailwindcss": {
      "command": "npx",
      "args": ["-y", "tailwindcss-mcp-server"]
    },
    "macro-mcp": {
      "command": "node",
      "args": ["dist/apps/macro-mcp/main.js"]
    }
  }
}
