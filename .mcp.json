{
  "mcpServers": {
    "ag-mcp": {
      "command": "npx",
      "args": ["ag-mcp"]
    },
    "primeng": {
      "command": "npx",
      "args": ["-y", "@primeng/mcp"]
    },
    "nx-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "nx-mcp@latest", "."]
    },
    "angular-cli": {
      "command": "npx",
      "args": ["-y", "@angular/cli", "mcp"]
    },
    "tailwindcss": {
      "command": "npx",
      "args": ["-y", "tailwindcss-mcp-server"]
    },
    "macro-mcp": {
      "command": "node",
      "args": ["E:/MacroDesktopMFE/macro/dist/apps/macro-mcp/main.js"]
    }
  }
}
